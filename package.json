{
  "name": "practica_final_plantilla",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "dayjs": "^1.9.7",
    "lodash.get": "^4.4.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
